{
  "name": "@liutsing/user-auth",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build --config tsconfig.build.json",
    "predeploy": "pnpm run build",
    "deploy-template": "node scripts/deploy.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@liutsing/enums": "^1.0.3",
    "@nestjs/common": "^10.3.3",
    "@nestjs/core": "^10.3.3",
    "@nestjs/microservices": "^10.3.3",
    "@nestjs/platform-express": "^10.3.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.0"
  },
  "devDependencies": {
    "@liutsing/eslint-config": "^0.0.11",
    "@liutsing/prettier-config": "^0.0.6",
    "@liutsing/tsconfig": "^0.0.6",
    "@liutsing/types-utils": "^1.0.2",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.3.3",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.2",
    "@types/supertest": "^2.0.11",
    "jest": "29.5.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prettier": "@liutsing/prettier-config"
}
