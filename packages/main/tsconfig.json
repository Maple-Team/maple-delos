{
  "extends": "@liutsing/tsconfig",
  "compilerOptions": {
    "baseUrl": "./",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "target": "ESnext",
    "outDir": "./dist",
    "rootDir": ".",
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "strictNullChecks": false,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*"
  ]
}