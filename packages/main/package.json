{
  "name": "@maple/delos-main",
  "version": "0.0.6",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "author": "",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production nest build --config tsconfig.build.json",
    "predeploy": "pnpm run build",
    "deploy": "node scripts/deploy.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prettier": "prettier . -w",
    "start": "cross-env NODE_ENV=development nest start",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "prettier": "@liutsing/prettier-config",
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@liutsing/utils": "^0.0.9",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/bull": "^10.0.0",
    "@nestjs/cache-manager": "^2.0.1",
    "@nestjs/common": "^10.0.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.3",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/microservices": "^10.0.3",
    "@nestjs/mongoose": "^10.0.0",
    "@nestjs/platform-express": "^10.0.3",
    "@nestjs/platform-socket.io": "^10.0.3",
    "@nestjs/platform-ws": "^10.0.3",
    "@nestjs/swagger": "^7.0.11",
    "@nestjs/terminus": "^10.2.0",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.0.3",
    "@socket.io/redis-adapter": "^8.2.1",
    "axios": "^1.6.7",
    "bull": "^4.9.0",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-store": "^3.0.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "lodash": "^4.17.21",
    "mongoose": "^7.3.1",
    "mqtt": "^4.3.7",
    "mysql2": "^3.4.2",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.1",
    "typeorm": "^0.3.17",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@liutsing/eslint-config": "^0.0.11",
    "@liutsing/prettier-config": "^0.0.6",
    "@liutsing/types-utils": "^0.1.0",
    "@nestjs/cli": "^10.0.5",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.3",
    "@types/bull": "^4.10.0",
    "@types/cache-manager": "^4.0.2",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.2",
    "@types/supertest": "^2.0.11",
    "@types/ws": "^8.5.5",
    "cross-env": "^7.0.3",
    "jest": "29.5.0",
    "rimraf": "^5.0.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  }
}
